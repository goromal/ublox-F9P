<launch>

<arg name="device" default="/dev/ttyACM0"/>

<!-- ZED-F9P Rover GPS -->
<node pkg="ublox_gps" type="ublox_gps" name="f9p_rover"
      output="screen"
      clear_params="false"
      respawn="true"
      respawn_delay="30">
  <param name="device" value="$(arg device)"/>
  <rosparam command="load"
            file="$(find ublox_gps)/config/f9p-rover.yaml" />
</node>

</launch>
